{
  "name": "homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --hot src/index.html",
    "build": "rm -rf build && bun build src/index.html --outdir build --minify --target browser --define.process.env.NODE_ENV=production",
    "serve": "bun run --production build/index.html"
  },
  "dependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
